+static int cmc623_i2c_read8(
+    u8 addr,
+    u32 *data)
+{
+
+	struct i2c_msg msg[2]; 
+	unsigned char in_data[1];		  
+	//int retry = 0;
+	int err;
+	
+	printk("[SHANKAR] %s [%d] ENTER", __func__, __LINE__);
+	
+	if (!sd->client->adapter)
+	{				 
+		return -ENODEV; 	   
+	}
+	
+	mutex_lock(&sd->xfer_lock);
+	in_data[0]= addr & 0xFF;
+	msg[0].addr = sd->client->addr;
+	msg[0].len = 1;
+	msg[0].flags = 0;
+	msg[0].buf = in_data;
+	
+	msg[1].addr = sd->client->addr;
+	msg[1].flags = I2C_M_RD;
+	msg[1].len = 1;
+	msg[1].buf = data;
+
+	
+	err = i2c_transfer(sd->client->adapter, msg, 2);
+	mutex_unlock(&sd->xfer_lock);
+	if (err > 0)
+        return 0;
+    else
+    {
+		printk("[SHANKAR] %s [%d] I2C write faild", __func__, __LINE__);
+		return -1;
+    }
+    
+}
